{
  "ConnectionStrings": {
    "Default": "Dummy connection string"
  },
  "App": {
    "ServerRootAddress": "https://api.kushal.kimbal.io/",
    "ClientRootAddress": "https://portal.kushal.kimbal.io/",
    "FirmwareRootAddress": "https://firmware.kushal.kimbal.io/",
    "CorsOrigins": "https://portal.kushal.kimbal.io,https://admin.kushal.kimbal.io,http://localhost:4200,https://firmware.kushal.kimbal.io"
  },
  "AWS": {
    "Firmware": {
      "AccessKeyId": "${FIRMWARE_S3_ACCESS_KEY_ID}",
      "SecretKey": "${FIRMWARE_S3_SECRET_KEY}",
      "Region": "ap-south-1",
      "S3BucketName": "${FIRMWARE_S3_BUCKET_NAME}"
    }
  },
  "ElectronAPI": {
    "Database": "ElectronAppDB"
  },
  "ErpAPI": {
    "BaseUrl": "${ERP_BASE_URL}",
    "Username": "${ERP_USERNAME}",
    "Password": "${ERP_PASSWORD}",
    "OrderListEndpoint": "/api/getManufactureOrderList",
    "OrderDetailsEndpoint": "/api/manufactureOrderDetails",
    "AuthenticateEndpoint": "/api/session/authenticate",
    "SendOrderEndpoint": "/api/Order/SendOrder"
  }
}
